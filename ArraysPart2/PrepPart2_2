// Smallest Value 
class Solution{
    public static int kthSmallest(int[] arr, int l, int r, int k) 
    { 
        Arrays.sort(arr);
        return arr[k-1];
    } 
}
// Largest Value
class Solution {
    public int findKthLargest(int[] nums, int k) {
       Arrays.sort(nums);
       return  nums[nums.length-k] ;
    }
}
